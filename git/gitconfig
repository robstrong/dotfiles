[core]
    editor = vim
    mergeoptions = --no-edit
    excludesfile = ~/.dotfiles/git/gitignore
[help]
    autocorrect = 1
[push]
    default = simple
[hub]
    protocol = ssh
[http]
    cookiefile = ~/.gitcookies
[alias]
    l = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset'
[commit]
	gpgsign = false
[include]
    path = ~/.gitconfig.private
